#!/mnt/c/Users/desck/src/obsidian-cli-helper/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'obsidian-pp-ocr-converter','console_scripts','obsidian-pp'
__requires__ = 'obsidian-pp-ocr-converter'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('obsidian-pp-ocr-converter', 'console_scripts', 'obsidian-pp')()
    )
